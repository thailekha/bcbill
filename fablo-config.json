{
  "$schema": "https://github.com/softwaremill/fablo/releases/download/1.0.2/schema.json",
  "global": {
    "fabricVersion": "2.3.0",
    "tls": false,
    "tools": {
      "explorer": false,
      "fabloRest": true
    }
  },
  "orgs": [
    {
      "organization": {
        "name": "Orderer",
        "domain": "orderer.example.com"
      },
      "orderers": [
        {
          "groupName": "group1",
          "type": "solo",
          "instances": 2
        }
      ]
    },
    {
      "organization": {
        "name": "Org1",
        "domain": "org1.example.com"
      },
      "peer": {
        "instances": 2,
        "db": "CouchDb"
      }
    },
  ],
  "channels": [
    {
      "name": "mychannel",
      "orgs": [
        {
          "name": "Org1",
          "peers": [
            "peer0", "peer1"
          ]
        },
      ]
    }
  ],
  "chaincodes": [
    {
      "name": "chaincode1",
      "endorsement": "AND('Org1MSP.member')",
      "version": "0.0.1",
      "lang": "node",
      "channel": "mychannel",
      "directory": "./chaincodes/chaincode-kv-node"
    }
  ]
}